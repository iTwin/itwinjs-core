  },
  "private": true,
  "scripts": {
    "build": "node ./scripts/setup-tests.js && tsc",
    "build:watch": "npm run build -- -w",
    "clean": "rimraf -g lib out .rush/temp/package-deps*.json",
    "docs": "npm run -s extract",
    "extract": "betools extract --fileExt=ts,tsx --extractFrom=./src --recursive --out=../../generated-docs/extract",
    "lint": "eslint \"./src/**/*.ts\" 1>&2",
    "test": "cross-env NODE_ENV=development mocha --config ./.mocharc.json --grep \"#performance\" --invert",
    "test:performance": "mocha --config ./.mocharc.json --grep \"#performance\" --no-timeouts",
    "copy:config": "internal-tools copy-config",
    "cover": "npm run -s test",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@itwin/build-tools": "workspace:*",
    "@itwin/core-backend": "workspace:*",
