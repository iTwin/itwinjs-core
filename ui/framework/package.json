{
  "name": "@bentley/ui-framework",
  "version": "2.10.0-dev.15",
  "description": "UI framework",
  "main": "lib/ui-framework.js",
  "typings": "lib/ui-framework",
  "imodeljsSharedLibrary": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs/tree/master/ui/framework"
  },
  "scripts": {
    "compile": "npm run build",
    "copy:assets": "cpx \"./src/**/*.{*css,json,svg}\" \"./lib\" && cpx \"./public/**/*\" \"./lib/public\"",
    "pseudolocalize": "betools pseudolocalize --englishDir ./public/locales/en --out ./lib/public/locales/en-PSEUDO",
    "build": "npm run copy:assets && npm run pseudolocalize && tsc 1>&2",
    "build:watch": "tsc --w",
    "clean": "rimraf lib .rush/temp/package-deps*.json",
    "cover": "nyc npm test",
    "docs": "betools docs --includes=../../generated-docs/extract --json=../../generated-docs/ui/ui-framework/file.json --tsIndexFile=./ui-framework.ts --onlyJson",
    "lint": "eslint -f visualstudio ./src/**/*.{ts,tsx} 1>&2",
    "extract-api": "betools extract-api --entry=ui-framework",
    "test": "mocha --opts ../mocha.opts \"./lib/test/**/*.test.js\"",
    "test:watch": "npm test -- --reporter min --watch-extensions ts,tsx --watch"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "peerDependencies": {
    "@bentley/bentleyjs-core": "^2.10.0-dev.15",
    "@bentley/frontend-authorization-client": "^2.10.0-dev.15",
    "@bentley/geometry-core": "^2.10.0-dev.15",
    "@bentley/imodelhub-client": "^2.10.0-dev.15",
    "@bentley/imodeljs-common": "^2.10.0-dev.15",
    "@bentley/imodeljs-frontend": "^2.10.0-dev.15",
    "@bentley/imodeljs-i18n": "^2.10.0-dev.15",
    "@bentley/imodeljs-markup": "^2.10.0-dev.15",
    "@bentley/itwin-client": "^2.10.0-dev.15",
    "@bentley/presentation-common": "^2.10.0-dev.15",
    "@bentley/presentation-frontend": "^2.10.0-dev.15",
    "@bentley/product-settings-client": "^2.10.0-dev.15",
    "@bentley/rbac-client": "^2.10.0-dev.15",
    "@bentley/ui-abstract": "^2.10.0-dev.15",
    "@bentley/ui-components": "^2.10.0-dev.15",
    "@bentley/ui-core": "^2.10.0-dev.15",
