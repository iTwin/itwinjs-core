{
  "name": "@bentley/presentation-frontend",
  "version": "2.0.0-dev.56",
  "description": "Frontend of iModel.js Presentation library",
  "main": "lib/presentation-frontend.js",
  "typings": "lib/presentation-frontend",
  "imodeljsSharedLibrary": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs/tree/master/presentation/frontend"
  },
  "keywords": [
    "Bentley",
    "EC",
    "Presentation",
    "iModelJS",
    "Frontend"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "scripts": {
    "copy:locale": "cpx \"../common/assets/**/*\" \"./lib/public\"",
    "copy:assets": "cpx \"./src/**/*.json\" \"./lib\"",
    "build": "npm run copy:assets && npm run copy:locale && npm run pseudolocalize && npm run extract && tsc 1>&2",
    "build:watch": "tsc -w",
    "clean": "rimraf lib .rush",
    "cover": "nyc npm test",
    "docs": "npm run docs:reference && npm run docs:changelog",
    "docs:changelog": "cpx \"./CHANGELOG.md\" ../../generated-docs/presentation/presentation-frontend",
    "docs:reference": "node ./node_modules/@bentley/build-tools/scripts/docs.js --includes=../../generated-docs/extract --json=../../generated-docs/presentation/presentation-frontend/file.json --tsIndexFile=presentation-frontend.ts --onlyJson %TYPEDOC_THEME%",
    "extract": "node ./node_modules/@bentley/build-tools/scripts/extract.js --fileExt=ts --extractFrom=./src/test --recursive --out=../../generated-docs/extract",
    "extract-api": "node ./node_modules/@bentley/build-tools/scripts/extract-api.js --entry=presentation-frontend",
    "lint": "npm run tslint && npm run eslint",
    "eslint": "eslint src --ext .ts,.js",
    "tslint": "tslint --project . 1>&2",
    "pseudolocalize": "node ./node_modules/@bentley/build-tools/scripts/pseudolocalize.js --englishDir ./lib/public/locales/en --out ./lib/public/locales/en-PSEUDO",
    "test": "mocha -r jsdom-global/register --opts ../mocha.opts \"./lib/test/**/*.test.js\"",
    "test:watch": "npm test -- --reporter min --watch-extensions ts --watch"
  },
  "dependencies": {
    "lodash": "^4.17.10"
  },
  "peerDependencies": {
    "@bentley/bentleyjs-core": "^2.0.0-dev.56",
    "@bentley/imodeljs-i18n": "^2.0.0-dev.56",
    "@bentley/imodeljs-common": "^2.0.0-dev.56",
    "@bentley/imodeljs-frontend": "^2.0.0-dev.56",
    "@bentley/presentation-common": "^2.0.0-dev.56"
  },
  "devDependencies": {
    "@bentley/bentleyjs-core": "2.0.0-dev.56",
    "@bentley/build-tools": "2.0.0-dev.56",
    "@bentley/imodeljs-i18n": "2.0.0-dev.56",
    "@bentley/imodeljs-common": "2.0.0-dev.56",
    "@bentley/imodeljs-frontend": "2.0.0-dev.56",
    "@bentley/imodeljs-clients": "2.0.0-dev.56",
    "@bentley/presentation-common": "2.0.0-dev.56",
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7",
    "@types/chai-jest-snapshot": "^1.3.0",
    "@types/deep-equal": "^1",
    "@types/faker": "^4.1.0",
    "@types/lodash": "^4.14.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "10.14.1",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.0",
    "@typescript-eslint/parser": "2.22.0",
    "cross-env": "^5.1.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7",
    "chai-jest-snapshot": "^2.0.0",
    "cpx": "^1.5.0",
    "deep-equal": "^1",
    "eslint": "^6.7.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "faker": "^4.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "typemoq": "^2.1.0",
    "typescript": "~3.7.4",
    "xmlhttprequest": "^1.8.0"
  },
  "nyc": {
    "extends": "./node_modules/@bentley/build-tools/.nycrc",
    "check-coverage": true,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "lines": 100,
    "require": [
      "jsdom-global/register"
    ]
  }
}
