  "peerDependencies": {
    "@bentley/bentleyjs-core": "^2.16.0-dev.3",
    "@bentley/itwin-client": "^2.16.0-dev.3"
  },
  "devDependencies": {
    "@bentley/bentleyjs-core": "2.16.0-dev.3",
    "@bentley/build-tools": "2.16.0-dev.3",
    "@bentley/eslint-plugin": "2.16.0-dev.3",
    "@bentley/itwin-client": "2.16.0-dev.3",
    "@types/node": "10.14.1",
    "eslint": "^6.8.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.0"
