    "@types/node": "14.14.31",
    "eslint": "^7.11.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.4.0"
  },
  "eslintConfig": {
    "plugins": [
      "@bentley"
    ],
    "extends": "plugin:@bentley/imodeljs-recommended"
