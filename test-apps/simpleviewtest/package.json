{
  "name": "simpleviewtest",
  "description": "Simple View Testing interactive application.",
  "license": "UNLICENSED",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "version": "0.0.0",
  "engines": {
    "node": "8.9.1"
  },
  "private": true,
  "scripts": {
    "build": "tsc 1>&2 && npm run copy:frontendpublic && npm run copy:ourpublic && npm run webpack:frontend",
    "clean": "rimraf lib package-deps.json",
    "copy:frontendpublic": "cpx \"../../core/frontend/src/public/**/*\" ./lib/backend/public",
    "copy:ourpublic": "cpx \"public/**/*\" ./lib/backend/public",
    "docs": "",
    "lint": "tslint --project . 1>&2",
    "mobile": "tsc 1>&2 && webpack --config mobile.backend.webpack.config.js 1>&2 && webpack --config mobile.frontend.webpack.config.js 1>&2 && cpx \"public/**/*\" ./lib/mobile/public && cpx \"assets/**/*\" ./lib/mobile/assets ",
    "start": "npm run start:electron",
    "start:dev-cors-proxy-server": "node ./node_modules/@bentley/dev-cors-proxy-server/server.js",
    "start:electron": "electron ./lib/backend/ElectronMain.js",
    "start:web": "node ./lib/backend/WebMain.js",
    "test": "",
    "webpack:frontend": "webpack --config webpack.config.js 1>&2"
  },
  "repository": {},
  "dependencies": {
    "@bentley/bentleyjs-core": "0.149.0",
    "@bentley/geometry-core": "0.149.0",
    "@bentley/imodeljs-clients": "0.149.0",
    "@bentley/imodeljs-common": "0.149.0",
    "@bentley/imodeljs-frontend": "0.149.0",
    "@bentley/imodeljs-backend": "0.149.0",
    "body-parser": "^1.18.2",
    "electron-devtools-installer": "^2.2.4",
    "tooltip.js": "^1.2.0"
  },
  "devDependencies": {
    "@bentley/config-loader": "0.149.0",
    "@bentley/build-tools": "0.149.0",
    "@bentley/dev-cors-proxy-server": "0.0.7",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.11.1",
    "@types/node": "10.10.3",
    "cpx": "^1.5.0",
    "child_process": "^1.0.2",
    "electron": "2.0.1",
    "electron-builder": "^20.13.3",
    "express": "^4.16.3",
    "node-glob": "^1.2.0",
    "null-loader": "^0.1.1",
    "popper.js": "^1.14.4",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.11.0",
    "typescript": "~3.0.0",
    "webpack": "^4.16.4"
  },
  "homepage": "http://localhost:3000/"
}