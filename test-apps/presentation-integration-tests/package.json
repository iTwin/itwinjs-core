{
  "name": "presentation-integration-tests",
  "version": "0.0.0",
  "description": "Presentation integration tests",
  "license": "UNLICENSED",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "private": true,
  "scripts": {
    "build": "npm run extract",
    "clean": "rimraf lib",
    "extract": "node ./node_modules/@bentley/bentleyjs-tools/scripts/extract.js --fileExt=ts --extractFrom=./src --recursive --out=../../generated-docs/extract",
    "lint": "tslint ./",
    "test": "cross-env TS_NODE_PROJECT=./tsconfig.json mocha -r ts-node/register -r tsconfig-paths/register -r source-map-support/register --check-leaks --full-trace --no-timeouts --file ./src/index.test.ts ./src/**/*.ts",
    "test-cover": "npm run test",
    "watch": "npm test -- --reporter min --watch-extensions ts --watch"
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "0.112.0",
