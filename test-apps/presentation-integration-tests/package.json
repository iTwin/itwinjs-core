  "scripts": {
    "build": "tsc 1>&2 && npm run extract",
    "clean": "rimraf lib",
    "docs": "",
    "extract": "node ./node_modules/@bentley/build-tools/scripts/extract.js --fileExt=ts --extractFrom=./src --recursive --out=../../generated-docs/extract",
