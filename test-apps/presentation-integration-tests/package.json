    "@bentley/imodeljs-common": "0.112.0",
    "@bentley/imodeljs-backend": "0.112.0",
    "@bentley/imodeljs-frontend": "0.112.0",
    "@bentley/presentation-common": "^0.5.1",
    "@bentley/presentation-backend": "^0.5.1",
    "@bentley/presentation-frontend": "^0.5.1",
    "@bentley/presentation-components": "^0.5.1",
    "@bentley/ui-core": "0.112.0",
    "@bentley/ui-components": "0.112.0",
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7",
    "@types/chai-jest-snapshot": "^1.3.0",
    "@types/chai-spies": "^0",
    "@types/cpx": "1.5",
    "@types/deep-equal": "^1",
    "@types/faker": "^4.1.0",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "chai-as-promised": "^7",
    "chai-jest-snapshot": "^2.0.0",
    "chai-spies": "1.0.0",
    "cpx": "^1.5.0",
    "deep-equal": "^1",
    "faker": "^4.1.0",
    "json-schema-faker": "^0.5.0-rc15",
    "mocha": "^5.2.0",
    "typemoq": "^2.1.0",
