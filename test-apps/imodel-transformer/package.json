{
  "name": "imodel-transformer",
  "//private": "NOTE: this is a private package that is never published",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "clean": "rimraf lib .rush/temp/package-deps*.json",
    "compile": "npm run -s build",
    "cover": "",
    "docs": "",
    "lint": "eslint --max-warnings 0 \"./src/**/*.ts\" 1>&2",
    "start": "node ./lib/Main.js",
    "test": "mocha"
  },
  "repository": {},
  "dependencies": {
    "@itwin/core-bentley": "workspace:*",
    "@itwin/core-electron": "workspace:*",
    "@itwin/core-geometry": "workspace:*",
    "@bentley/imodelhub-client": "workspace:*",
    "@itwin/core-common": "workspace:*",
    "@itwin/core-backend": "workspace:*",
    "@itwin/core-transformer": "workspace:*",
    "@bentley/itwin-client": "workspace:*",
    "fs-extra": "^8.1.0",
    "yargs": "^16.0.0"
  },
  "devDependencies": {
    "@itwin/build-tools": "workspace:*",
    "@itwin/eslint-plugin": "workspace:*",
    "@bentley/itwin-registry-client": "workspace:*",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^8.2.2",
    "@types/fs-extra": "^4.0.7",
    "@types/lodash": "^4.14.0",
    "@types/node": "14.14.31",
    "@types/yargs": "^12.0.5",
    "eslint": "^7.11.0",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "typescript": "~4.4.0"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": "plugin:@itwin/itwinjs-recommended"
  }
}