{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Main.js",
      "program": "${workspaceRoot}/lib/Main.js",
      "cwd": "${workspaceRoot}",
      "args": [
        "--input=${workspaceFolder}/data/Neighborhoods_Philadelphia.geojson",
        "--output=${workspaceFolder}/lib/Neighborhoods_Philadelphia.bim"
      ],
      "smartStep": true,
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/lib/**/*.js"
      ],
      "protocol": "inspector"
    }
  ]
}
