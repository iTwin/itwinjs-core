{
  "name": "ui-test-app",
  "description": "UI Test Application",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run build:copy-config && npm run build:src",
    "build:src": "node ./node_modules/@bentley/webpack-tools/bin/buildIModelJsModule",
    "build:copy-config": "cpx ./src/backend/web/BackendServer.config.json ./lib/backend/web",
    "lint": "tslint -p . 1>&2",
    "electron": "electron ./lib/backend/main.js",
    "start:webserver": "node ./node_modules/@bentley/imodeljs-webserver/lib/WebServer.js --port=3000 --resources=./lib/webresources/",
    "start:backend": "node --max-http-header-size=16000 lib/backend/main.js",
    "start:servers": "run-p \"start:webserver\" \"start:backend\"",
    "clean": "rimraf lib dist package-deps.json",
    "test": "",
    "cover": "",
    "docs": ""
  },
  "iModelJs": {
    "buildModule": {
      "type": "application",
      "sourceResources": [
        {
          "source": "./src/**/*.scss",
          "dest": "./lib"
        },
        {
          "source": "./src/**/*.svg",
          "dest": "./lib"
        },
        {
          "source": "./public/**/*",
          "dest": "./lib/webresources"
        },
        {
          "source": "./src/frontend/plugins/public/**/*",
          "dest": "./lib/webresources"
        }
      ],
      "webpack": {
        "dest": "./lib/webresources",
        "entry": "./lib/frontend/index.js",
        "bundleName": "main",
        "styleSheets": true,
        "htmlTemplate": "./src/frontend/index.html"
      },
      "subModules": [
        {
          "dest": "./lib/webresources",
          "entry": "./lib/frontend/plugins/MeasurePoints.js",
          "bundleName": "MeasurePoints",
          "type": "plugin"
        }
      ],
      "makeConfig": {
        "dest": "./lib/webresources/config.json",
        "filter": "^imjs_"
      },
      "pseudoLocalize": {
        "source": "./lib/webresources/locales/en",
        "dest": "./lib/webresources/locales/en-pseudo"
      }
    }
  },
  "repository": {},
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "//devDependencies": [
    "NOTE: All tools used by scripts in this package must be listed as devDependencies"
  ],
  "devDependencies": {
    "@bentley/config-loader": "0.191.0-dev.30",
    "@bentley/logger-config": "0.191.0-dev.30",
    "@bentley/webpack-tools": "0.191.0-dev.30",
    "@bentley/build-tools": "0.191.0-dev.30",
    "@bentley/bentleyjs-core": "0.191.0-dev.30",
    "@bentley/express-server": "0.191.0-dev.30",
    "@bentley/imodeljs-i18n": "0.191.0-dev.30",
    "@bentley/imodeljs-backend": "0.191.0-dev.30",
    "@bentley/imodeljs-frontend": "0.191.0-dev.30",
    "@bentley/imodeljs-quantity": "0.191.0-dev.30",
    "@bentley/imodeljs-common": "0.191.0-dev.30",
    "@bentley/imodeljs-clients": "0.191.0-dev.30",
    "@bentley/imodeljs-webserver": "0.191.0-dev.30",
    "@bentley/icons-generic-webfont": "^0.0.9",
    "@types/classnames": "^2.2.3",
    "@types/node": "10.14.1",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/semver": "^5.5.0",
    "autoprefixer": "^8.6.5",
    "css-loader": "^0.28.11",
    "electron": "^4.1.0",
    "electron-chromedriver": "^2.0.0",
    "file-loader": "^1.1.11",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "react-test-renderer": "^16.4.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "svg-sprite-loader": "^3.8.0",
    "tsconfig-paths": "^3.3.2",
    "tslint": "^5.11.0",
    "typescript": "~3.2.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.20.2"
  },
  "dependencies": {
