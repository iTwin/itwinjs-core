{
  "name": "testbed",
  "//private": "NOTE: this is a private package that is never published",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rush build -t testbed",
    "build": "tsc 1>&2 && npm run copy:frontendpublic && npm run copy:testpublic && npm run webpack:frontend",
    "build:perf": "npm run prebuild && tsc 1>&2 && npm run copy:frontendpublic && npm run copy:testpublic && npm run webpack:frontend:perf",
    "clean": "rimraf lib backend/lib frontend/lib package-deps.json",
    "copy:frontendpublic": "cpx ../../core/frontend/src/public/**/* ./lib/backend/public",
    "copy:testpublic": "cpx ./public/**/* ./lib/backend/public",
    "cover": "node bin/testbed.js --coveragePattern ../core/frontend/lib/**/*.js",
    "docs": "",
    "lint": "tslint --project . 1>&2",
    "webpack:frontend:perf": "webpack --config ./webpack_performance.config.js 1>&2",
    "webpack:frontend": "webpack --config webpack.config.js 1>&2",
    "runExcel": "tsc && node ./lib/frontend/performance/PerformanceReportWriter.js",
    "test": "node bin/testbed.js --fgrep #integration --invert",
    "test:debug": "node bin/testbed.js --debug --fgrep #integration --invert",
    "test:integration": "node bin/testbed.js --fgrep #integration",
    "test:integration-debug": "node bin/testbed.js --debug --fgrep #integration",
    "test:performance": "npm run build:perf && node bin/testbed.js --usePerfWriterServer"
  },
  "repository": {},
  "dependencies": {
    "@bentley/bentleyjs-core": "0.127.0",
    "@bentley/geometry-core": "0.127.0",
    "@bentley/imodeljs-backend": "0.127.0",
    "@bentley/imodeljs-clients": "0.127.0",
    "@bentley/imodeljs-common": "0.127.0",
    "@bentley/imodeljs-i18n": "0.127.0",
    "@bentley/imodeljs-frontend": "0.127.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "commander": "^2.14.1",
    "electron": "2.0.1",
    "express": "^4.16.3",
    "fs-extra": "^6.0.1",
    "fuse.js": "^3.2.0",
    "i18next": "^10.2.2",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "js-base64": "^2.4.5",
    "save": "^2.3.2"
  },
  "devDependencies": {
    "@bentley/bentleyjs-tools": "0.127.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/exceljs": "^0.5.2",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^4.0.7",
    "@types/i18next": "^8.4.2",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/i18next-xhr-backend": "^1.4.1",
    "@types/js-base64": "^2.3.1",
    "@types/mocha": "^5.2.5",
    "@types/webpack": "^3.8.5",
    "chai": "^4.1.2",
    "cpx": "^1.5.0",
    "exceljs": "^1.5.1",
    "mocha": "^5.2.0",
    "node-glob": "^1.2.0",
    "null-loader": "^0.1.1",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.11.0",
    "typescript": "~3.0.0",
    "webpack": "^4.16.4"
  }
}
