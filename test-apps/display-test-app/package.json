    "@bentley/webgl-compatibility": "2.0.0-dev.55",
    "body-parser": "^1.18.2",
    "tooltip.js": "^1.2.0"
  },
  "devDependencies": {
    "@bentley/config-loader": "2.0.0-dev.55",
