    "@types/react-dom": "^16.0.6",
    "electron": "2.0.1",
    "electron-builder": "^20.13.3",
    "webpack": "^4.16.4",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~2.9.1"
  },
  "proxy": {
