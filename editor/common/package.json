{
  "name": "@itwin/editor-common",
  "version": "5.0.0-dev.3",
  "description": "iTwin.js editing properties common to frontend and backend",
  "main": "lib/cjs/editor-common.js",
  "module": "lib/esm/editor-common.js",
  "typings": "lib/cjs/editor-common",
  "license": "MIT",
  "scripts": {
    "build": "npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "clean": "rimraf lib .rush/temp/package-deps*.json",
    "docs": "betools docs --includes=../../generated-docs/extract --json=../../generated-docs/editor/editor-common/file.json --tsIndexFile=./editor-common.ts --onlyJson",
    "extract-api": "betools extract-api --entry=editor-common",
    "lint": "eslint --max-warnings 0 ./src/**/*.ts 1>&2",
    "test": "",
    "cover": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/itwinjs-core.git",
    "directory": "editor/common"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "digital-twin",
    "iTwin"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "peerDependencies": {
    "@itwin/core-bentley": "workspace:^5.0.0-dev.3",
    "@itwin/core-geometry": "workspace:^5.0.0-dev.3",
    "@itwin/core-common": "workspace:^5.0.0-dev.3"
  },
  "devDependencies": {
    "@itwin/core-bentley": "workspace:*",
    "@itwin/build-tools": "workspace:*",
    "@itwin/eslint-plugin": "5.0.0-dev.1",
    "@itwin/core-geometry": "workspace:*",
    "@itwin/core-common": "workspace:*",
    "@types/chai": "4.3.1",
    "@types/mocha": "^10.0.6",
    "chai": "^4.3.10",
    "eslint": "^9.13.0",
    "mocha": "^10.2.0",
    "rimraf": "^3.0.2",
    "typescript": "~5.6.2"
  }
}
