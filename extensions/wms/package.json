{
  "name": "wms-extension-example",
  "version": "1.0.0",
  "description": "Demonstration WMS Server",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run pseudolocalize && npm run copy:assets",
    "build": "npm run prebuild && extension-webpack-tools build -s ./src/wmsExtension.ts -o ./lib/extension",
    "clean": "rimraf lib .rush",
    "copy:assets": "cpx \"./src/public/locales/**/*\" ./lib/extension/locales/",
    "cover": "",
    "docs": "",
    "lint": "tslint -p . 1>&2",
    "pseudolocalize": "betools pseudolocalize --englishDir=./src/public/locales/en --out=./lib/extension/locales/en-PSEUDO",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs/tree/master/extensions/wms"
  },
  "keywords": [
    "iModel",
    "BIM",
    "wms",
    "extension"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/imodeljs/wms-example-plugin#readme",
  "devDependencies": {
    "@bentley/build-tools": "2.0.0-dev.57",
    "@bentley/extension-webpack-tools": "2.0.0-dev.57",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.2",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "typescript": "~3.7.4"
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "2.0.0-dev.57",
    "@bentley/geometry-core": "2.0.0-dev.57",
    "@bentley/imodeljs-clients": "2.0.0-dev.57",
    "@bentley/imodeljs-common": "2.0.0-dev.57",
    "@bentley/imodeljs-i18n": "2.0.0-dev.57",
    "@bentley/imodeljs-frontend": "2.0.0-dev.57",
    "@bentley/imodeljs-quantity": "2.0.0-dev.57"
  }
}
