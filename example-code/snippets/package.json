    "@bentley/imodeljs-clients": "1.0.0-dev.4",
    "@bentley/imodeljs-clients-backend": "1.0.0-dev.4",
    "@bentley/imodeljs-common": "1.0.0-dev.4",
    "@bentley/imodeljs-frontend": "1.0.0-dev.4",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "commander": "^2.14.1",
    "electron": "^4.1.0",
    "express": "^4.16.3",
    "fs-extra": "^6.0.1",
    "fuse.js": "^3.3.0",
    "i18next": "^10.2.2",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^2.0.1",
    "js-base64": "^2.4.5",
    "save": "^2.3.3",
    "webpack": "^4.20.2"
  },
  "devDependencies": {
    "@bentley/config-loader": "1.0.0-dev.4",
    "@bentley/build-tools": "1.0.0-dev.4",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^4.0.7",
    "@types/i18next": "^8.4.2",
    "@types/i18next-browser-languagedetector": "^2.0.1",
