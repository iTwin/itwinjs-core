{
  "name": "example-code-app",
  "//private": "NOTE: this is a private package that is never published",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm run build-code && npm run extract && npm run extract-assets",
    "build-code": "tsc 1>&2",
    "clean": "rimraf lib package-deps.json ../../generated-docs",
    "extract-assets": "cpx ./src/backend/assets/**/* ../../generated-docs/extract",
    "copy:backend-assets": "cpx ./src/backend/assets/**/* ./lib/backend/assets",
    "copy:test-backend-assets": "cpx ./src/backend/test/assets/**/* ./lib/backend/test/assets",
    "extract": "node ./node_modules/@bentley/bentleyjs-tools/scripts/extract.js --fileExt=ts --extractFrom=./src --recursive --out=../../generated-docs/extract",
    "docs": "",
    "lint": "tslint --project . 1>&2",
    "pretest": "npm run copy:test-backend-assets && npm run copy:backend-assets && npm run build-code",
    "test": "node ./node_modules/@bentley/bentleyjs-tools/scripts/test.js --testDir=\"./lib/backend/test\""
  },
  "repository": {},
  "dependencies": {
    "@bentley/bentleyjs-core": "0.98.0-1",
    "@bentley/geometry-core": "0.98.0-1",
    "@bentley/imodeljs-backend": "0.98.0-1",
    "@bentley/imodeljs-clients": "0.98.0-1",
    "@bentley/imodeljs-common": "0.98.0-1",
    "@bentley/imodeljs-frontend": "0.98.0-1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "commander": "^2.14.1",
    "electron": "2.0.1",
    "express": "^4.16.3",
    "fs-extra": "^6.0.1",
    "fuse.js": "^3.2.0",
    "i18next": "^10.2.2",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "js-base64": "^2.4.5",
    "save": "^2.3.2",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "@bentley/bentleyjs-tools": "0.98.0-1",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^4.0.7",
    "@types/i18next": "^8.4.2",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/i18next-xhr-backend": "^1.4.1",
    "@types/js-base64": "^2.3.1",
    "@types/mocha": "^2.2.47",
