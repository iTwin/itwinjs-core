{
  "name": "@bentley/frontend-devtools",
  "version": "2.0.0-dev.55",
  "description": "Debug menu and supporting UI widgets",
  "main": "lib/frontend-devtools.js",
  "imodeljsSharedLibrary": true,
  "license": "MIT",
  "scripts": {
    "build": "npm run copy:assets && tsc 1>&2",
    "copy:assets": "cpx ./public/**/* ./lib/public",
    "clean": "rimraf lib .rush",
    "docs": "node ./node_modules/@bentley/build-tools/scripts/docs.js --includes=../../generated-docs/extract --json=../../generated-docs/core/frontend-devtools/file.json --tsIndexFile=./frontend-devtools.ts --onlyJson %TYPEDOC_THEME%",
    "extract-api": "node ./node_modules/@bentley/build-tools/scripts/extract-api.js --entry=frontend-devtools",
    "lint": "tslint --project . 1>&2",
    "test": "",
    "cover": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs/tree/master/core/frontend-devtools"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "UI",
    "Widget"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "dependencies": {
    "@bentley/imodeljs-frontend": "2.0.0-dev.55",
    "@bentley/imodeljs-i18n": "2.0.0-dev.55",
    "@bentley/imodeljs-common": "2.0.0-dev.55",
    "@bentley/bentleyjs-core": "2.0.0-dev.55",
    "@bentley/geometry-core": "2.0.0-dev.55",
    "file-saver": "^2.0.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@bentley/build-tools": "2.0.0-dev.55",
    "@types/node": "10.14.1",
    "@types/file-saver": "^2.0.1",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.2",
    "typescript": "~3.7.4",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2"
  },
  "nyc": {
    "nycrc-path": "./node_modules/@bentley/build-tools/.nycrc",
    "all": true
  }
}