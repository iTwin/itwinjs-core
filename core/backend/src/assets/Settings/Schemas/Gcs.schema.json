{
  "$id": "http://itwinjs.org/json-schema/gcs",
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "GeoCoordinate settings",
  "description": "settings for geocoordinate services",
  "type": "object",
  "groupName": "itwin/core/gcs",
  "properties": {
    "itwin/core/gcs/default/databases": {
      "$ref": "http://itwinjs.org/json-schema/workspace#/workspace/databases",
      "description": "List of GCS workspace databases to load by default.",
      "default": [
        {
          "name": "itwin/gcs/base",
          "dbName": "base",
          "version": "^1",
          "baseUri": "https://geocoord-workspace.itwinjs.org",
          "containerId": "gcs",
          "isPublic": true,
          "priority": 10000,
          "prefetch": true
        },
        {
          "name": "itwin/gcs/entire-earth",
          "dbName": "allEarth",
          "version": "^1",
          "baseUri": "https://geocoord-workspace.itwinjs.org",
          "containerId": "gcs",
          "priority": 100
        }
      ]
    }
  },
  "itwin/core/gcs/disableWorkspaces": {
    "type": "boolean",
    "description": "If true, do not load any GCS workspaces",
    "default": false
  }
}