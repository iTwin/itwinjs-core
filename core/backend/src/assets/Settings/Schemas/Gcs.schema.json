{
  "$schema": "itwin-settings/schema/1.1",
  "description": "settings for geocoordinate services",
  "groupName": "itwin/core/gcs",
  "settingDefs": {
    "default/databases": {
      "type": "array",
      "description": "array of GCS WorkspaceDbs",
      "items": {
        "type": "object",
        "extends": "itwin/core/gcs/GcsWorkspaceDb",
        "description": "Properties of a WorkspaceDb holding GCS data.",
        "properties": {
          "priority": {
            "type": "number",
            "description": "Priority value for GCS database, higher values are searched first",
            "default": 500
          }
        }
      },
      "default": [
        {
          "dbName": "base",
          "version": "^1",
          "baseUri": "https://geocoord-workspace.itwinjs.org",
          "containerId": "gcs",
          "isPublic": true,
          "priority": 10000,
          "prefetch": true
        },
        {
          "dbName": "allEarth",
          "version": "^1",
          "baseUri": "https://geocoord-workspace.itwinjs.org",
          "containerId": "gcs",
          "isPublic": true,
          "priority": 100
        }
      ]
    },
    "disableWorkspaces": {
      "type": "boolean",
      "description": "If true, do not load any GCS workspaces",
      "default": false
    }
  }
}