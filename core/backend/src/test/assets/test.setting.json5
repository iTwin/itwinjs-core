{
    "editor/fontWeight": "normal",
    "editor/fontSize": 12,
    "editor/multiCursorModifier": "ctrlCmd",
    "editor/cursorStyle": "line",
    "git/enableSmartCommit": true,
    "git/confirmSync": false,
    "alignment/surroundSpace": {
        "colon": [
            -1,
            1
        ], // The first number specify how much space to add to the left, can be negative.
        // The second number is how much space to the right, can be negative.
        "assignment": [
            1,
            1
        ], // The same as above.
        "arrow": [
            1,
            1
        ], // The same as above.
        "comment": 2 // Special how much space to add between the trailing comment and the code.
        // If this value is negative, it means don't align the trailing comment.
    },
    "window/restoreWindows": "all",
    "window/restoreFullscreen": true,
    "cSpell/enableFiletypes": [
        "c",
        "cpp",
        "csharp",
        "go",
        "html",
        "javascript",
        "javascriptreact",
        "latex",
        "markdown",
        "php",
        "plaintext",
        "python",
        "text",
        "typescript",
        "typescriptreact",
        "xml",
        "yml"
    ],
    "editor/renderWhitespace": "selection",
    "editor/lineHeight": 11,
    "editor/fontLigatures": true,
    "workbench/colorCustomizations": {
        "Visual Studio Light": {
            "editor/lineHighlightBorder": "#2422222a",
            "editor/background": "#ece2c5",
            "sideBar.background": "#e2daca"
        }
    },
    "editor/tokenColorCustomizations": {
        "Default High Contrast": {
            "comments": "#FF0000"
        },
        "Visual Studio Light": {
            "types": "#9c3569",
            "variables": "#7e5007",
            "textMateRules": [
                {
                    "scope": [
                        "constant",
                        "constant.language",
                        "support.constant",
                    ],
                    "settings": {
                        "foreground": "#d16c6c",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": [
                        "comment",
                    ],
                    "settings": {
                        "foreground": "#7c7b7b",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "variable.language",
                    ],
                    "settings": {
                        "foreground": "#9c7325"
                    }
                },
                {
                    "scope": [
                        "entity.name.class",
                        "entity.name.section",
                        "entity.name.selector",
                        "entity.name.tag",
                        "entity.name.type",
                        "entity.other",
                        "entity",
                    ],
                    "settings": {
                        "foreground": "#0857be"
                    }
                },
                {
                    "scope": [
                        "keyword.control",
                        "keyword.operator.arithmetic",
                        "keyword.operator.assignment",
                        "keyword.operator.logical",
                        "keyword.operator.new",
                        "keyword.operator",
                        "keyword.other",
                        "keyword",
                    ],
                    "settings": {
                        "foreground": "#068135",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": [
                        "storage",
                        "storage.type",
                        "storage.modifier"
                    ],
                    "settings": {
                        "foreground": "#723a3a",
                        "fontStyle": "bold"
                    }
                }
            ]
        }
    },
    "workbench/colorTheme": "Visual Studio Light",
    "editor/codeActionsOnSave": null,
    "window/zoomLevel": 1,
    "app1/tree/blah": "blah default"
}