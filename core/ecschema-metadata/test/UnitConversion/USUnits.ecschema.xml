<?xml version="1.0" encoding="utf-8"?>
<ECSchema schemaName="USUnits" alias="USU" version="01.00.05" description="Standard Set of Unit definitions for the system" displayLabel="Units" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.2">
