{
  "name": "@bentley/express-server",
  "version": "1.4.0",
  "description": "iModel.js express utilities",
  "main": "lib/ExpressServer.js",
  "typings": "lib/ExpressServer",
  "license": "MIT",
  "engines": {
    "node": ">=10.11.0 <11.0"
  },
  "scripts": {
    "build": "tsc 1>&2 && node ./node_modules/@bentley/build-tools/scripts/ignoreargs.js 1>&2",
    "clean": "rimraf lib package-deps.json",
    "docs": "node ./node_modules/@bentley/build-tools/scripts/docs.js --source=./src --includes=../../generated-docs/extract --json=../../generated-docs/core/express-server/file.json --tsIndexFile=./ExpressServer.ts --onlyJson %TYPEDOC_THEME%",
    "extract-api": "node ./node_modules/@bentley/build-tools/scripts/extract-api.js --entry=ExpressServer",
    "lint": "tslint --project . 1>&2",
    "test": "",
    "cover": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@bentley/build-tools": "1.4.0",
    "@bentley/imodeljs-common": "1.4.0",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/node": "10.14.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "typescript": "~3.5.2"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3"
  }
}
