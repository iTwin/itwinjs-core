<ECSchema schemaName="AecUnits" alias="AECU" version="01.00.03" description="This contains the unit definitions that are use across AE Schemas" displayLabel="Design Modeling Units" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.2">
    <ECSchemaReference name="Formats" version="01.00.00" alias="f"/>
    <ECSchemaReference name="Units"   version="01.00.03" alias="u"/>

    <KindOfQuantity typeName="ACCELERATION"                      displayLabel="Acceleration"                      persistenceUnit="u:M_PER_SEC_SQ"                      relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:M_PER_SEC_SQ];f:DefaultRealU(4)[u:CM_PER_SEC_SQ];f:DefaultRealU(4)[u:FT_PER_SEC_SQ]"/>
    <KindOfQuantity typeName="ANGLE"                             displayLabel="Angle"                             persistenceUnit="u:RAD"                               relativeError="0.0001" presentationUnits="f:DefaultRealU(2)[u:ARC_DEG];f:AngleDMS"/>
    <KindOfQuantity typeName="ANGULAR_VELOCITY"                  displayLabel="Angular Velocity"                  persistenceUnit="u:RAD_PER_SEC"                       relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:RAD_PER_SEC];f:DefaultRealU(4)[u:DEG_PER_SEC];f:DefaultRealU(4)[u:RPM]"/>
    <KindOfQuantity typeName="AREA"                              displayLabel="Area"                              persistenceUnit="u:SQ_M"                              relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:SQ_M];f:DefaultRealU(4)[u:SQ_FT]"/>
    <KindOfQuantity typeName="AREA_LARGE"                        displayLabel="Large Area"                        persistenceUnit="u:SQ_M"                              relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:SQ_KM];f:DefaultRealU(4)[u:SQ_MILE]"/>
    <KindOfQuantity typeName="AREA_SMALL"                        displayLabel="Small Area"                        persistenceUnit="u:SQ_M"                              relativeError="0.0001" presentationUnits="f:DefaultRealU(2)[u:SQ_MM];f:DefaultRealU(4)[u:SQ_IN]"/>
    <KindOfQuantity typeName="AREA_FORCE"                        displayLabel="Area Force"                        persistenceUnit="u:PA"                                relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:KILOPASCAL];f:DefaultRealU(4)[u:PSF]"/>
    <KindOfQuantity typeName="AREA_FORCE_LARGE"                  displayLabel="Area Force Large"                  persistenceUnit="u:PA"                                relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:MEGAPASCAL];f:DefaultRealU(4)[u:KSF]"/>
    <KindOfQuantity typeName="AREA_FORCE_SMALL"                  displayLabel="Area Force Small"                  persistenceUnit="u:PA"                                relativeError="0.0001" presentationUnits="f:DefaultRealU(2)[u:PA];f:DefaultRealU(4)[u:PSF]"/>
    <KindOfQuantity typeName="AREA_MOMENT"                       displayLabel="Area Moment"                       persistenceUnit="u:N_M_PER_SQ_M"                      relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:KN_M_PER_SQ_M];f:DefaultRealU(4)[u:LBF_FT_PER_SQ_FT]" />
    <KindOfQuantity typeName="AREA_MOMENT_LARGE"                 displayLabel="Area Moment Large"                 persistenceUnit="u:N_M_PER_SQ_M"                      relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:MEGAN_M_PER_SQ_M];f:DefaultRealU(4)[u:KPF_FT_PER_SQ_FT]" />
    <KindOfQuantity typeName="AREA_MOMENT_SMALL"                 displayLabel="Area Moment Small"                 persistenceUnit="u:N_M_PER_SQ_M"                      relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:N_M_PER_SQ_M];f:DefaultRealU(4)[u:LBF_IN_PER_SQ_IN]" />
    <KindOfQuantity typeName="AREA_SPRING_CONSTANT"              displayLabel="Area Spring Constant"              persistenceUnit="u:AREA_SPRING_CONSTANT_N_PER_CUB_M"  relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:AREA_SPRING_CONSTANT_N_PER_CUB_M];f:DefaultRealU(4)[u:AREA_SPRING_CONSTANT_KN_PER_CUB_M];f:DefaultRealU(4)[u:AREA_SPRING_CONSTANT_KPF_PER_CUB_FT]" />

    <KindOfQuantity typeName="CURRENT"                           displayLabel="Current"                           persistenceUnit="u:A"                                 relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:A];f:DefaultRealU(4)[u:KILOAMPERE]"/>

    <KindOfQuantity typeName="DENSITY"                           displayLabel="Density"                           persistenceUnit="u:KG_PER_CUB_M"                      relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:KG_PER_CUB_M];f:DefaultRealU(4)[u:LBM_PER_CUB_FT];f:DefaultRealU(4)[u:G_PER_CUB_CM];f:DefaultRealU(4)[u:LBM_PER_CUB_IN];f:DefaultRealU(4)[u:KIP_PER_CUB_FT]"/>
    <KindOfQuantity typeName="DYNAMIC_VISCOSITY"                 displayLabel="Dynamic Viscosity"                 persistenceUnit="u:PA_S"                              relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:PA_S];f:DefaultRealU(4)[u:POISE];f:DefaultRealU(4)[u:CENTIPOISE];f:DefaultRealU(4)[u:LBM_PER_FT_S]"/>

    <KindOfQuantity typeName="ELECTRIC_POTENTIAL"                displayLabel="Electric Potential"                persistenceUnit="u:VOLT"                              relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:VOLT];f:DefaultRealU(4)[u:KILOVOLT];f:DefaultRealU(4)[u:MEGAVOLT]"/>
    <KindOfQuantity typeName="ENERGY"                            displayLabel="Energy"                            persistenceUnit="u:J"                                 relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:J];f:DefaultRealU(4)[u:KJ];f:DefaultRealU(4)[u:BTU];f:DefaultRealU(4)[u:KWH]"/>

    <KindOfQuantity typeName="FLOW"                              displayLabel="Flow Rate"                         persistenceUnit="u:CUB_M_PER_SEC"                     relativeError="1e-05"  presentationUnits="f:DefaultRealU(4)[u:LITRE_PER_MIN];f:DefaultRealU(4)[u:CUB_FT_PER_MIN];f:DefaultRealU(4)[u:GALLON_PER_MIN]"/>
    <KindOfQuantity typeName="FORCE"                             displayLabel="Force"                             persistenceUnit="u:N"                                 relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:KN];f:DefaultRealU(4)[u:LBF]"/>
    <KindOfQuantity typeName="FORCE_LARGE"                       displayLabel="Force Large"                       persistenceUnit="u:N"                                 relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:MEGAN];f:DefaultRealU(4)[u:KPF]"/>
    <KindOfQuantity typeName="FORCE_SMALL"                       displayLabel="Force Small"                       persistenceUnit="u:N"                                 relativeError="0.0001" presentationUnits="f:DefaultRealU(2)[u:N];f:DefaultRealU(4)[u:LBF]"/>
    <KindOfQuantity typeName="FORCE_DENSITY"                     displayLabel="Force Density"                     persistenceUnit="u:N_PER_CUB_M"                       relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:N_PER_CUB_M];f:DefaultRealU(4)[u:KN_PER_CUB_M];f:DefaultRealU(4)[u:N_PER_CUB_FT];f:DefaultRealU(4)[u:KN_PER_CUB_FT]"/>
    <KindOfQuantity typeName="FREQUENCY"                         displayLabel="Frequency"                         persistenceUnit="u:HZ"                                relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:HZ];f:DefaultRealU(4)[u:KHZ];f:DefaultRealU(4)[u:MHZ]"/>

    <KindOfQuantity typeName="HEAT_TRANSFER"                     displayLabel="Heat Transfer"                     persistenceUnit="u:W_PER_SQ_M_K"                      relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:W_PER_SQ_M_K];f:DefaultRealU(4)[u:W_PER_SQ_M_CELSIUS];f:DefaultRealU(4)[u:BTU_PER_SQ_FT_HR_FAHRENHEIT]"/>

    <KindOfQuantity typeName="ILLUMINANCE"                       displayLabel="Illuminance"                       persistenceUnit="u:LUX"                               relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:LUX];f:DefaultRealU(4)[u:LUMEN_PER_SQ_FT]"/>

    <KindOfQuantity typeName="LENGTH"                            displayLabel="Length"                            persistenceUnit="u:M"                                 relativeError="1e-05"  presentationUnits="f:DefaultRealU(4)[u:M];f:DefaultRealU(4)[u:MM];f:AmerFI;f:AmerFI(16);f:DefaultRealU(4)[u:FT]"/>
    <KindOfQuantity typeName="LENGTH_LONG"                       displayLabel="Long Length"                       persistenceUnit="u:M"                                 relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:KM];f:DefaultRealU(4)[u:M];f:AmerFI;f:AmerFI(16);f:DefaultRealU(4)[u:FT]"/>
    <KindOfQuantity typeName="LENGTH_SHORT"                      displayLabel="Short Length"                      persistenceUnit="u:M"                                 relativeError="0.01"   presentationUnits="f:DefaultRealU(2)[u:MM];f:DefaultRealU(4)[u:CM];f:AmerFI;f:AmerFI(16);f:DefaultRealU(4)[u:IN]"/>
    <KindOfQuantity typeName="LINEAR_DENSITY"                    displayLabel="Linear Density"                    persistenceUnit="u:KG_PER_M"                          relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:KG_PER_M];f:DefaultRealU(4)[u:KG_PER_MM];f:DefaultRealU(4)[u:LBM_PER_FT]"/>
    <KindOfQuantity typeName="LINEAR_FORCE"                      displayLabel="Linear Force"                      persistenceUnit="u:N_PER_M"                           relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:KN_PER_M];f:DefaultRealU(4)[u:LBF_PER_FT]"/>
    <KindOfQuantity typeName="LINEAR_FORCE_LARGE"                displayLabel="Linear Force Large"                persistenceUnit="u:N_PER_M"                           relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:MEGAN_PER_M];f:DefaultRealU(4)[u:KPF_PER_FT]"/>
    <KindOfQuantity typeName="LINEAR_FORCE_SMALL"                displayLabel="Linear Force Small"                persistenceUnit="u:N_PER_M"                           relativeError="0.0001" presentationUnits="f:DefaultRealU(2)[u:N_PER_M];f:DefaultRealU(4)[u:LBF_PER_FT]"/>
    <KindOfQuantity typeName="LINEAR_MOMENT"                     displayLabel="Linear Moment"                     persistenceUnit="u:N_M_PER_M"                         relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:KN_M_PER_M];f:DefaultRealU(4)[u:LBF_FT_PER_FT]" />
    <KindOfQuantity typeName="LINEAR_MOMENT_LARGE"               displayLabel="Linear Moment Large"               persistenceUnit="u:N_M_PER_M"                         relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:MEGAN_M_PER_M];f:DefaultRealU(4)[u:KPF_FT_PER_FT]" />
    <KindOfQuantity typeName="LINEAR_MOMENT_SMALL"               displayLabel="Linear Moment Small"               persistenceUnit="u:N_M_PER_M"                         relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:N_M_PER_M];f:DefaultRealU(4)[u:LBF_FT_PER_FT]" />
    <KindOfQuantity typeName="LINEAR_SPRING_CONSTANT"            displayLabel="Linear Spring Constant"            persistenceUnit="u:LINEAR_SPRING_CONSTANT_N_PER_SQ_M" relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:LINEAR_SPRING_CONSTANT_N_PER_SQ_M];f:DefaultRealU(4)[u:LINEAR_SPRING_CONSTANT_KN_PER_SQ_M];f:DefaultRealU(4)[u:LINEAR_SPRING_CONSTANT_KPF_PER_SQ_FT]" />
    <KindOfQuantity typeName="LINEAR_ROTATIONAL_SPRING_CONSTANT" displayLabel="Linear Rotational Spring Constant" persistenceUnit="u:N_PER_RAD"                         relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:N_PER_RAD];f:DefaultRealU(4)[u:KN_PER_RAD];f:DefaultRealU(4)[u:KPF_PER_RAD]"/>
    <KindOfQuantity typeName="LIQUID_VOLUME"                     displayLabel="Liquid Volume"                     persistenceUnit="u:CUB_M"                             relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:LITRE];f:DefaultRealU(4)[u:GALLON]"/>
    <KindOfQuantity typeName="LIQUID_VOLUME_LARGE"               displayLabel="Liquid Large Volume"               persistenceUnit="u:CUB_M"                             relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:THOUSAND_LITRE];f:DefaultRealU(4)[u:THOUSAND_GALLON]"/>
    <KindOfQuantity typeName="LIQUID_VOLUME_SMALL"               displayLabel="Liquid Small Volume"               persistenceUnit="u:CUB_M"                             relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:LITRE];f:DefaultRealU(4)[u:GALLON]"/>
    <KindOfQuantity typeName="LUMINOUS_FLUX"                     displayLabel="Luminous Flux"                     persistenceUnit="u:LUMEN"                             relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:LUMEN]"/>
    <KindOfQuantity typeName="LUMINOUS_INTENSITY"                displayLabel="Luminous Intensity"                persistenceUnit="u:CD"                                relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:CD]"/>
 
    <KindOfQuantity typeName="MOMENT"                            displayLabel="Moment"                            persistenceUnit="u:N_M"                               relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:KN_M];f:DefaultRealU(4)[u:LBF_FT]" />
    <KindOfQuantity typeName="MOMENT_LARGE"                      displayLabel="Moment Large"                      persistenceUnit="u:N_M"                               relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:MEGAN_M];f:DefaultRealU(4)[u:KPF_FT]" />
    <KindOfQuantity typeName="MOMENT_SMALL"                      displayLabel="Moment Small"                      persistenceUnit="u:N_M"                               relativeError="0.0001" presentationUnits="f:DefaultRealU(2)[u:N_M];f:DefaultRealU(4)[u:LBF_FT]" />
    <KindOfQuantity typeName="MOMENT_OF_INERTIA"                 displayLabel="Moment Of Inertia"                 persistenceUnit="u:M_TO_THE_FOURTH"                   relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:M_TO_THE_FOURTH];f:DefaultRealU(4)[u:MM_TO_THE_FOURTH];f:DefaultRealU(4)[u:CM_TO_THE_FOURTH];f:DefaultRealU(4)[u:IN_TO_THE_FOURTH];f:DefaultRealU(4)[u:FT_TO_THE_FOURTH]"/>

    <KindOfQuantity typeName="POWER"                             displayLabel="Power"                             persistenceUnit="u:W"                                 relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:W];f:DefaultRealU(4)[u:KW];f:DefaultRealU(4)[u:MEGAW];f:DefaultRealU(4)[u:BTU_PER_HR];f:DefaultRealU(4)[u:KILOBTU_PER_HR];f:DefaultRealU(4)[u:HP]"/>
    <KindOfQuantity typeName="PRESSURE"                          displayLabel="Pressure"                          persistenceUnit="u:PA"                                relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:PA];f:DefaultRealU(4)[u:PSIG];f:DefaultRealU(4)[u:PSI]"/>
    <KindOfQuantity typeName="PRESSURE_GRADIENT"                 displayLabel="Pressure Gradient"                 persistenceUnit="u:PA_PER_M"                          relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:PA_PER_M];f:DefaultRealU(4)[u:BAR_PER_KM]"/>
    <KindOfQuantity typeName="PROBABILITY"                       displayLabel="Probability"                       persistenceUnit="u:PROBABILITY_FRACTION"              relativeError="0.001"  presentationUnits="f:DefaultRealU(2)[u:PROBABILITY_PERCENT]"/>
    <KindOfQuantity typeName="PROCESS_PIPING_FLOW"               displayLabel="Process Piping Flow Rate"          persistenceUnit="u:CUB_M_PER_SEC"                     relativeError="1e-05"  presentationUnits="f:DefaultRealU(4)[u:LITRE_PER_MIN];f:DefaultRealU(4)[u:CUB_FT_PER_MIN];f:DefaultRealU(4)[u:GALLON_PER_MIN]"/>
    <KindOfQuantity typeName="PROCESS_PIPING_PRESSURE"           displayLabel="Process Piping Pressure"           persistenceUnit="u:PA"                                relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:PA];f:DefaultRealU(4)[u:PSIG];f:DefaultRealU(4)[u:PSI]"/>
    <KindOfQuantity typeName="PROCESS_PIPING_TEMPERATURE"        displayLabel="Process Piping Temperature"        persistenceUnit="u:K"                                 relativeError="0.01"   presentationUnits="f:DefaultRealU(2)[u:CELSIUS];f:DefaultRealU(2)[u:FAHRENHEIT];f:DefaultRealU(4)[u:K]"/>

    <KindOfQuantity typeName="ROTATIONAL_SPRING_CONSTANT"        displayLabel="Rotational Spring Constant"        persistenceUnit="u:N_M_PER_RAD"                       relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:N_M_PER_RAD];f:DefaultRealU(4)[u:KN_M_PER_RAD];f:DefaultRealU(4)[u:KPF_FT_PER_RAD]"/>

    <KindOfQuantity typeName="SPECIFIC_HEAT_CAPACITY"            displayLabel="Specific Heat Capacity"            persistenceUnit="u:J_PER_KG_K"                        relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:J_PER_KG_K];f:DefaultRealU(4)[u:BTU_PER_LBM_RANKINE]"/>
    <KindOfQuantity typeName="SPECIFIC_HEAT_OF_VAPORIZATION"     displayLabel="Specific Heat Of Vaporization"     persistenceUnit="u:J_PER_KG"                          relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:J_PER_KG];f:DefaultRealU(4)[u:KJ_PER_KG];f:DefaultRealU(4)[u:BTU_PER_LBM]"/>
    <KindOfQuantity typeName="SPRING_CONSTANT"                   displayLabel="Spring Constant"                   persistenceUnit="u:SPRING_CONSTANT_N_PER_M"           relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:SPRING_CONSTANT_N_PER_M];f:DefaultRealU(4)[u:SPRING_CONSTANT_KN_PER_M];f:DefaultRealU(4)[u:SPRING_CONSTANT_KPF_PER_FT]"/>

    <KindOfQuantity typeName="TEMPERATURE"                       displayLabel="Temperature"                       persistenceUnit="u:K"                                 relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:CELSIUS];f:DefaultRealU(4)[u:FAHRENHEIT];f:DefaultRealU(4)[u:K]"/>
    <KindOfQuantity typeName="THERMAL_CONDUCTIVITY"              displayLabel="Thermal Conductivity"              persistenceUnit="u:W_PER_M_K"                         relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:W_PER_M_K];f:DefaultRealU(4)[u:W_PER_M_C];f:DefaultRealU(4)[u:BTU_IN_PER_SQ_FT_HR_FAHRENHEIT]"/>
    <KindOfQuantity typeName="THERMAL_EXPANSION_COEFFICIENT"     displayLabel="Thermal Expansion Coefficient"     persistenceUnit="u:STRAIN_PER_KELVIN"                 relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:STRAIN_PER_KELVIN];f:DefaultRealU(4)[u:STRAIN_PER_CELSIUS];f:DefaultRealU(4)[u:STRAIN_PER_FAHRENHEIT]"/>
    <KindOfQuantity typeName="THERMAL_RESISTANCE"                displayLabel="Thermal Resistance"                persistenceUnit="u:SQ_M_KELVIN_PER_WATT"              relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:SQ_M_KELVIN_PER_WATT];f:DefaultRealU(4)[u:SQ_M_CELSIUS_PER_WATT];f:DefaultRealU(4)[u:SQ_FT_HR_FAHRENHEIT_PER_BTU]"/>
    <KindOfQuantity typeName="TIME"                              displayLabel="Time"                              persistenceUnit="u:S"                                 relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:S];f:DefaultRealU(4)[u:MIN];f:DefaultRealU(4)[u:HR];f:DefaultRealU(4)[u:DAY];f:DefaultRealU(4)[u:MS]"/>

    <KindOfQuantity typeName="VELOCITY"                          displayLabel="Velocity"                          persistenceUnit="u:M_PER_SEC"                         relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:M_PER_SEC];f:DefaultRealU(4)[u:FT_PER_SEC];f:DefaultRealU(4)[u:MPH];f:DefaultRealU(4)[u:KM_PER_HR]"/>
    <KindOfQuantity typeName="VOLUME"                            displayLabel="Volume"                            persistenceUnit="u:CUB_M"                             relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:CUB_M];f:DefaultRealU(4)[u:CUB_FT];f:DefaultRealU(4)[u:CUB_YRD]"/>
    <KindOfQuantity typeName="VOLUME_LARGE"                      displayLabel="Large Volume"                      persistenceUnit="u:CUB_M"                             relativeError="0.01"   presentationUnits="f:DefaultRealU(4)[u:CUB_KM];f:DefaultRealU(4)[u:CUB_M];f:DefaultRealU(4)[u:CUB_YRD];f:DefaultRealU(4)[u:CUB_FT]"/>
    <KindOfQuantity typeName="VOLUME_SMALL"                      displayLabel="Small Volume"                      persistenceUnit="u:CUB_M"                             relativeError="0.0001" presentationUnits="f:DefaultRealU(4)[u:CUB_MM];f:DefaultRealU(4)[u:CUB_CM];f:DefaultRealU(4)[u:CUB_IN];f:DefaultRealU(4)[u:CUB_FT]"/>
    
    <KindOfQuantity typeName="WARPING_CONSTANT"                  displayLabel="Warping Constant"                  persistenceUnit="u:M_TO_THE_SIXTH"                    relativeError="0.001"  presentationUnits="f:DefaultRealU(4)[u:M_TO_THE_SIXTH];f:DefaultRealU(4)[u:FT_TO_THE_SIXTH]"/>
    <KindOfQuantity typeName="WEIGHT"                            displayLabel="Weight"                            persistenceUnit="u:KG"                                relativeError="0.001"  presentationUnits="f:DefaultRealU(2)[u:KG];f:DefaultRealU(2)[u:LBM]"/> 
 </ECSchema>
