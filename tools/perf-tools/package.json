    "@itwin/eslint-plugin": "workspace:*",
    "@types/fs-extra": "^4.0.7",
    "eslint": "^7.11.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.4.0"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
