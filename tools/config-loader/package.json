{
  "name": "@bentley/config-loader",
  "version": "2.10.0-dev.16",
  "description": "Configuration loader",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs/tree/master/tools/config-loader"
  },
  "typings": "lib/index",
  "scripts": {
    "compile": "npm run build",
    "build": "tsc 1>&2",
    "clean": "rimraf lib .rush/temp/package-deps*.json",
    "docs": "",
    "lint": "eslint -f visualstudio ./src/**/*.ts 1>&2",
    "extract-api": "betools extract-api --entry=index",
    "test": "",
    "cover": ""
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "json5": "^2.1.0"
  },
  "devDependencies": {
    "@bentley/build-tools": "2.10.0-dev.16",
    "@bentley/eslint-plugin": "2.10.0-dev.16",
    "@types/json5": "0.0.30",
    "@types/node": "10.14.1",
    "eslint": "^6.8.0",
    "rimraf": "^3.0.2",
    "typescript": "~3.7.4"
  },
  "eslintConfig": {
    "plugins": [
      "@bentley"
    ],
    "extends": "plugin:@bentley/imodeljs-recommended",
    "rules": {
      "@typescript-eslint/camelcase": "off",
      "no-console": "off",
      "no-eval": "off"
    }
  }
}
